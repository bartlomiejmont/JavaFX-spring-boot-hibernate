<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ogloszenia.controller.UserViewController">
   <children>
      <VBox alignment="CENTER" layoutX="125.0" prefHeight="402.0" prefWidth="350.0" spacing="20.0">
         <children>
            <Button fx:id="jobsButton" mnemonicParsing="false" onAction="#jobsClick" prefWidth="100.0" text="Jobs">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button fx:id="messagesButton" mnemonicParsing="false" prefWidth="100.0" text="Messages" onAction="#messagesClick">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button fx:id="itemsButton" mnemonicParsing="false" prefWidth="100.0" text="Items" onAction="#itemsClick" />
            <Button fx:id="rentalsButton" mnemonicParsing="false" prefWidth="100.0" text="Rentals" onAction="#rentalsClick"/>
            <Button fx:id="housesButton" mnemonicParsing="false" prefWidth="100.0" text="Houses" onAction="#housesClick"/>
            <Button fx:id="opinionsButton" mnemonicParsing="false" prefWidth="100.0" text="Opinions" onAction="#opinionsClick" />
         </children>
      </VBox>
   </children>
</AnchorPane>
